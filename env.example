# Network Configuration
RPC_URL=https://evm-rpc.sei-apis.com
CHAIN_ID=1329
NETWORK_TYPE=testnet

# Wallet Configuration
PRIVATE_KEY=your_wallet_private_key_here
EXECUTOR_PRIVATE_KEY=your_executor_private_key_here
EXECUTOR_ADDRESS=your_executor_address_here

# Console Kit Configuration
BRAHMA_API_KEY=your_brahma_console_api_key_here
REGISTRY_ID=your_registry_id_here

# AI Configuration (Required for sei-agent-kit)
OPENAI_API_KEY=your_openai_api_key_here

# Automation Settings
POLLING_INTERVAL=30000
AUTO_REBALANCE_ENABLED=true
MAX_SLIPPAGE=0.5
MIN_YIELD_THRESHOLD=5.0
RISK_TOLERANCE=medium
MAX_POSITION_SIZE=10000
MAX_POSITION_SIZE_USD=50000
REBALANCE_THRESHOLD=2.0
EMERGENCY_STOP_LOSS=10.0
MAX_GAS_PRICE=1000000000

# Protocol Preferences
PREFERRED_PROTOCOLS=symphony,takara,silo
MAX_PROTOCOL_EXPOSURE=30
MIN_HEALTH_FACTOR=1.5
MAX_LEVERAGE=3.0

# Logging Configuration
LOG_LEVEL=info
LOG_FILE=logs/sei-defi-agent.log

# API Server Configuration (Optional)
PORT=3000
HOST=localhost

# Security Configuration
ENCRYPTION_KEY=your_encryption_key_here
SESSION_SECRET=your_session_secret_here

# Monitoring & Alerts (Optional)
DISCORD_WEBHOOK_URL=your_discord_webhook_url_here
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
TELEGRAM_CHAT_ID=your_telegram_chat_id_here
SLACK_WEBHOOK_URL=your_slack_webhook_url_here

# External API Keys (Optional)
COINGECKO_API_KEY=your_coingecko_api_key_here
DEXSCREENER_API_KEY=your_dexscreener_api_key_here

# Performance Tracking
ENABLE_METRICS=true
METRICS_RETENTION_DAYS=30

# Emergency Controls
ENABLE_EMERGENCY_STOP=true
MAX_DAILY_LOSS=1000
CIRCUIT_BREAKER_THRESHOLD=20

# Development/Testing
NODE_ENV=production
DEBUG=false
DRY_RUN=false 