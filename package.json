{
  "name": "sei-defi-agent",
  "version": "1.0.0",
  "description": "Automated DeFi agent for Sei blockchain with yield optimization and risk management",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "register-executor": "tsx src/scripts/register-executor.ts",
    "deploy-account": "tsx src/scripts/deploy-automation-account.ts",
    "run-agent": "tsx src/automation/agent-workflow.ts",
    "update-automation": "tsx src/scripts/update-automation.ts",
    "cancel-automation": "tsx src/scripts/cancel-automation.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "sei",
    "defi",
    "agent",
    "automation",
    "yield-optimization",
    "console-kit",
    "blockchain"
  ],
  "author": "Sei DeFi Agent Team",
  "license": "MIT",
  "dependencies": {
    "brahma-console-kit": "^1.0.19",
    "ethers": "^6.13.5",
    "ethers-multisend": "^3.1.0",
    "viem": "^2.23.11",
    "axios": "^1.7.9",
    "ws": "^8.18.0",
    "dotenv": "^16.4.7",
    "node-cron": "^3.0.3",
    "winston": "^3.15.0",
    "joi": "^17.13.3",
    "bignumber.js": "^9.1.2",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/node": "^22.10.7",
    "@types/ws": "^8.5.13",
    "@types/node-cron": "^3.0.11",
    "@types/lodash": "^4.17.13",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.6",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
} 