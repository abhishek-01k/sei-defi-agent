{
  "name": "sei-defi-agent",
  "version": "1.0.0",
  "description": "AI-powered DeFi automation agent for Sei blockchain using sei-agent-kit",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "register-executor": "tsx src/scripts/register-executor.ts",
    "deploy-account": "tsx src/scripts/deploy-automation-account.ts",
    "run-agent": "tsx src/automation/agent-workflow.ts",
    "update-automation": "tsx src/scripts/update-automation.ts",
    "cancel-automation": "tsx src/scripts/cancel-automation.ts",
    "example": "tsx examples/basic-usage.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "sei",
    "defi",
    "agent",
    "automation",
    "yield-optimization",
    "console-kit",
    "blockchain",
    "ai",
    "langchain",
    "sei-agent-kit"
  ],
  "author": "Sei DeFi Agent Team",
  "license": "MIT",
  "dependencies": {
    "@bancor/carbon-sdk": "^0.0.115-DEV",
    "@cosmjs/stargate": "^0.33.0",
    "@langchain/core": "^0.3.19",
    "@langchain/langgraph": "^0.2.65",
    "@langchain/openai": "^0.5.10",
    "axios": "^1.7.9",
    "bignumber.js": "^9.1.2",
    "brahma-console-kit": "^1.0.19",
    "citrex-sdk": "^1.2.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "ethers": "^6.13.5",
    "ethers-multisend": "^3.1.0",
    "express": "^4.19.2",
    "joi": "^17.13.3",
    "langchain": "^0.3.19",
    "lodash": "^4.17.21",
    "node-cron": "^3.0.3",
    "openai": "^3.3.0",
    "symphony-sdk": "^2.1.0",
    "twitter-api-v2": "^1.24.0",
    "viem": "^2.23.11",
    "winston": "^3.15.0",
    "ws": "^8.18.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/node": "^22.10.7",
    "@types/ws": "^8.5.13",
    "@types/node-cron": "^3.0.11",
    "@types/lodash": "^4.17.13",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.6",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}